<LPHEADER>
<TABLE border="1" cellpadding="6" cellspacing="2"
  summary="Summary of recent %site% error/warning events">
  <TR>
    <TH colspan="8" scope="colgroup">%site% error/warning events</TH>
  </TR>
  <TR>
    <TH scope="col">Date</TH>
    <TH scope="col">Time</TH>
    <TH scope="col">Source</TH>
    <TH scope="col">Server</TH>
    <TH scope="col">Client IP</TH>
    <TH scope="col">Status</TH>
    <TH scope="col">Cookie</TH>
    <TH scope="col">Error</TH>
  </TR>
</LPHEADER>
<LPBODY>
<TR>
    <TD scope="row">%date%</TD>
    <TD>%time%</TD>
    <TD>%s-sitename%</TD>
    <TD>%ComputerName%</TD>
    <TD>%c-ip%</TD>
    <TD>%sc-status%</TD>
    <TD>%cs-cookie%</TD>
    <TD>%Message%</TD>
  </TR>
</LPBODY>
<LPFOOTER>
</TABLE>
</LPFOOTER>
